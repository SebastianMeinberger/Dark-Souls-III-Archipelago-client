{
  "version": 8,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "include": [],
  "configurePresets": [
	{
      "name": "mingw",
      "displayName": "mingw with vcpkg",
      "description": "Default build using Unix Makefiles and vcpkg",
      "generator": "Unix Makefiles",
	  "binaryDir": "${sourceDir}/build/vcpkg",
      "cacheVariables": {
			"VCPKG_CMAKE_SYSTEM_NAME": "mingw",
			"CMAKE_SYSTEM_NAME": "mingw",
			"CMAKE_SYSTEM_PROCESSOR": "x86_64",
			"CMAKE_C_COMPILER": "x86_64-w64-mingw32-gcc",
			"CMAKE_CXX_COMPILER": "x86_64-w64-mingw32-g++",
			"CMAKE_ASM_MASM_COMPILER": "uasm",
			"CMAKE_RC_COMPILER": "x86_64-w64-mingw32-windres",
			"CMAKE_MC_COMPILER": "x86_64-w64-mingw32-windmc",
			"CMAKE_FIND_ROOT_PATH": "/usr/x86_64-w64-mingw32",
			"(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM": "NEVER",
			"VCPKG_TARGET_TRIPLET": "x64-mingw-static",
			"CMAKE_EXPORT_COMPILE_COMMANDS": "on"
			},
      "environment": {},
      "vendor": {}
	}
],
  "buildPresets": [],
  "testPresets": [],
  "packagePresets": [],
  "workflowPresets": [],
  "vendor": {}
}
